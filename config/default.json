{
  "dirs": {
    "source": "./source",
    "cache": "./cache",
    "mount": "./mount"
  },

  "preload": {
    "filter": "^.+\\.flac$",
    "openDelay": 2000,
    "readPercentage": 50,
    "siblings": 3
  },

  "cleanup": {
    "cleanAfter": 21600,
    "cleanFrequency": 21600,
    "ignoreFilter": "^.*[^\\d-](1[-0])?0?1\\.flac$"
  },

  "fuseOptions": [],

  "queueLimit": 20,

  "logLevel": "warn"

}
